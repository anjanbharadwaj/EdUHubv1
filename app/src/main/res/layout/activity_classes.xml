<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_classes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#000000"
    tools:context="com.example.anjanbharadwaj.agendaplusapp.ClassesActivity"
    android:orientation="vertical"
    android:weightSum="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >


        <Button
            android:layout_width="48dp"
            android:layout_height="50dp"
            android:text=""
            android:background="@drawable/setting"
            android:layout_marginRight="8dp"
            android:id="@+id/buttonsettings" />


        <Button
            android:layout_width="38dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/signout"
            android:background="@drawable/power"
            android:onClick="signout"
            android:layout_marginRight="8dp" />

        <Button
            android:layout_width="46dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/calendar"
            android:background="@drawable/calendar"
            android:layout_marginRight="8dp"
            />

        <Button
            android:layout_width="42dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/homework"
            android:background="@drawable/homework"
            android:layout_marginRight="8dp"
            />


        <Button
            android:layout_width="44dp"
            android:layout_height="50dp"
            android:text=""
            android:background="@drawable/study"
            android:layout_marginRight="8dp"


            android:id="@+id/study" />

        <Button
            android:layout_height="50dp"
            android:id="@+id/classes"
            android:background="@drawable/classes"
            android:layout_width="50dp" />

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/notes"
            android:background="@drawable/notes"
            android:layout_marginRight="8dp"
            /><![CDATA[







            />




    ]]>
    </LinearLayout>


</LinearLayout>
